<!doctype html>
<html lang="fr">
    <head>
        {# Required meta tags #}
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        {# Title #}
        <title>{% block title %}Cerema{% endblock %}</title>
        {# SVG Icon #}
        <link rel="icon" href="{{ asset('images/svg/cerema.svg') }}" sizes="any" type="image/svg+xml">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
